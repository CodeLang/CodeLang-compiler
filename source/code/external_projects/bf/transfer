#!/bin/zsh
source ~/.zshrc
unilang --input_files redis_queue.unilang --exporter UBER --languages JAVA --style NORMAL --build-only;
cp RedisQueue.java ~/Desktop/buildfarm/src/main/java/build/buildfarm/instance/shard/queues/; 
java -jar ~/Desktop/buildfarm/java_formatter.jar -i ~/Desktop/buildfarm/src/main/java/build/buildfarm/instance/shard/queues/RedisQueue.java

unilang --input_files redis_provision_queue.unilang --exporter UBER --languages JAVA --style NORMAL --build-only;
cp RedisProvisionQueue.java ~/Desktop/buildfarm/src/main/java/build/buildfarm/instance/shard/queues/; 
java -jar ~/Desktop/buildfarm/java_formatter.jar -i ~/Desktop/buildfarm/src/main/java/build/buildfarm/instance/shard/queues/RedisProvisionQueue.java

unilang --input_files redis_provision_queues.unilang --exporter UBER --languages JAVA --style NORMAL --build-only;
cp RedisProvisionQueues.java ~/Desktop/buildfarm/src/main/java/build/buildfarm/instance/shard/queues/; 
java -jar ~/Desktop/buildfarm/java_formatter.jar -i ~/Desktop/buildfarm/src/main/java/build/buildfarm/instance/shard/queues/RedisProvisionQueues.java