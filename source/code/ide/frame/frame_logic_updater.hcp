◀public: static▶ Frame_Elements ☀each_frame(Ide_Settings & settings)❰
    Frame_Elements elements;
    
    //the order that the frame elements are added, are the order they are drawed.
    //this will affect what is drawn on top of / under other elements.
    
    //draw the background
    elements.types.emplace_back(settings.background.main_window_backdrop);
    
    //draw the content model
    elements.types += Label_Frame_Getter::Get(settings).types;
    
    //draw the mouse
    elements.types += Mouse_Frame_Getter::Get(settings).types;
    
    return elements;
