◀public: static▶ void ☀Draw_Everything(sf::RenderWindow & window, Game_State & state, Assets & assets) ❰

    for (auto const& it: state.board){
        window.draw(it);
    }
    
    window.draw(assets.player1_name);
    window.draw(assets.player2_name);
   
    for (auto const& it: state.player_1_choices){
        window.draw(it);
    }
    
    for (auto const& it: state.player_2_choices){
        window.draw(it);
    }

