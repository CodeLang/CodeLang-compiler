#!/usr/bin/env bash
rm -rf $HOME/.bashrc
cp -rf .bashrc $HOME/.bashrc

rm -rf $HOME/.bash_profile
cp -rf .bash_profile $HOME/.bash_profile

rm -rf $HOME/.bash_logout
cp -rf .bash_logout $HOME/.bash_logout

rm -rf $HOME/.bashrc_impl
cp -rf .bashrc_impl $HOME/.bashrc_impl

echo "installed bash configuration"

bash;
