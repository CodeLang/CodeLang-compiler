#include <iostream>
#include <fstream>
#include <sstream>
using namespace std;

string GetNextString(string *str)
{
	string str2;
	int pos;
	pos = str->find(" ");
	str2 = str->substr(pos+1, str->length());
	cout << str2 << endl;
	return str2;
}

string GetNextData(string *str)
{
	string str2;
	int pos;
	pos = str->find(" ");
	str2 = str->substr(1, pos);
	cout << str2 << endl;
	return str2;
}

int main() {
	
	string line;
	cin >> line;
	
	cout << "XYZDane:" << endl;
	cout << line << endl;
	
	GetNextData(*line);
	line = GetNextString(*line);
	
	GetNextData(*line);
	line = GetNextString(*line);
	
	return 0;
}