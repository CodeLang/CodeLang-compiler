◀public: static▶ std::vector<int> ☀Switch_Branch_And_Do_Profiles(std::string const& branch_name, std::string const& file_name) ❰

    
    //switch to that branch
    Branch_Switcher::Switch(branch_name);
    
    int trials = 5;
    
    std::vector<std::string> results;
    for (int i = 0; i < trials; ++i){
        auto profile_time = Profile_Compilation_Timer::Profile(file_name);
        
        std::cout << i+1 << "/" << trials << " " << branch_name << " " << profile_time << std::endl;
        results.emplace_back(profile_time);
    }
    
    return VecStringToVecInt(results);
