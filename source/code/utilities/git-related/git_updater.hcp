◀public: static▶ void ☀Update_All_Changes(std::string const& message)❰

    std::string command = "git add -A :/;";
    execute(command);
    
    command = "git commit -am \"";
    command += message;
    command += "\"";
    execute(command);
    
    command = "git pull;";
    execute(command);
    
    command = "git push;";
    execute(command);
    
