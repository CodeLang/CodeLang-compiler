◀public: static▶ unsigned int ☀Convert(std::string const& rfc333_datetime)❰
    using namespace date;
    std::istringstream infile{rfc333_datetime};
    sys_seconds tp;
    infile >> parse("%FT%T%Ez", tp);
    auto result = tp.time_since_epoch();
    
    return result.count();
