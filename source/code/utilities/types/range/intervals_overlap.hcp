◀public: static▶ bool ☀Check(Range const& a, Range const& b)❰

    if (a.low >= b.low && a.low <= b.high){
        return true;
    }
    
    if (a.high <= b.high && a.high >= b.low){
        return true;
    }
    
    if (b.low >= a.low && b.low <= a.high){
        return true;
    }
    
    if (b.high <= a.high && b.high >= a.low){
        return true;
    }

    return false;

